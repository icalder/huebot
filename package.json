{
  "name": "huebot",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "dev": "ts-node-esm --experimental-specifier-resolution=node server.ts",
    "build": "npm run build:client && npm run build:server && npm run build:express",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.ts",
    "build:express": "tsc --p ./tsconfig-express.json",
    "preview": "vite preview",
    "serve": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/express/server"
  },
  "dependencies": {
    "chart.js": "^4.0.1",
    "compression": "^1.7.4",
    "dotenv": "^16.0.3",
    "eventsource": "^2.0.2",
    "express": "^4.18.2",
    "mongodb": "4.11",
    "nocache": "^3.0.4",
    "node-hue-api": "^5.0.0-beta.14",
    "vue": "^3.2.37",
    "vue-chartjs": "^5.0.1",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/eventsource": "^1.1.9",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "cross-env": "^7.0.3",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.1.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vitest": "^0.25.5",
    "vue-tsc": "^0.40.4"
  }
}
